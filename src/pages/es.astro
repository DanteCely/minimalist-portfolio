---
import Layout from '@layouts/Layout.astro';
import Resume from '@components/Resume.astro';
import cvLang from '@utils/cv-lang';

const lang = 'es';
const resume = await cvLang(lang);

const { hero } = resume;
---

<Layout title={`${hero.title} - ${hero.role}`} lang={lang}>
	<Resume resume={resume} />
</Layout>
